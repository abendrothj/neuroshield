{
  "name": "neurashield-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest --projects=backend",
    "test:frontend": "jest --projects=frontend",
    "test:integration": "jest test/",
    "test:all": "npm run test:backend && npm run test:frontend && npm run test:integration",
    "perf:analyze": "cross-env ANALYZE=true next build",
    "perf:lighthouse": "lighthouse http://localhost:3000 --view",
    "perf:bundle": "next build && source-map-explorer .next/static/**/*.js"
  },
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "fabric-network": "^2.2.20",
    "next": "14.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.0.0",
    "source-map-explorer": "^2.5.3",
    "supertest": "^7.1.1",
    "typescript": "^5.3.0"
  }
}
