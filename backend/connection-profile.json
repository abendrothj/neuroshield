{
  "name": "neurashield-network",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300",
          "eventHub": "300",
          "eventReg": "300"
        },
        "orderer": "300"
      }
    },
    "discovery": {
      "enabled": true,
      "asLocalhost": false
    },
    "tlsEnable": true,
    "tlsClientCerts": {
      "verifyHostname": true
    }
  },
  "channels": {
    "neurashield-channel": {
      "orderers": [
        "orderer.neurashield.com"
      ],
      "peers": {
        "peer0.org1.neurashield.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1.org1.neurashield.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer0.org2.neurashield.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1.org2.neurashield.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.neurashield.com",
        "peer1.org1.neurashield.com"
      ],
      "certificateAuthorities": [
        "ca.org1.neurashield.com"
      ]
    },
    "Org2": {
      "mspid": "Org2MSP",
      "peers": [
        "peer0.org2.neurashield.com",
        "peer1.org2.neurashield.com"
      ],
      "certificateAuthorities": [
        "ca.org2.neurashield.com"
      ]
    },
    "OrdererOrg": {
      "mspid": "OrdererMSP",
      "orderers": [
        "orderer.neurashield.com"
      ]
    }
  },
  "orderers": {
    "orderer.neurashield.com": {
      "url": "grpcs://orderer.neurashield.com:7050",
      "grpcOptions": {
        "ssl-target-name-override": "orderer.neurashield.com"
      },
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    }
  },
  "peers": {
    "peer0.org1.neurashield.com": {
      "url": "grpcs://peer0.org1.neurashield.com:7051",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org1.neurashield.com"
      },
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    },
    "peer1.org1.neurashield.com": {
      "url": "grpcs://peer1.org1.neurashield.com:8051",
      "grpcOptions": {
        "ssl-target-name-override": "peer1.org1.neurashield.com"
      },
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    },
    "peer0.org2.neurashield.com": {
      "url": "grpcs://peer0.org2.neurashield.com:9051",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org2.neurashield.com"
      },
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    },
    "peer1.org2.neurashield.com": {
      "url": "grpcs://peer1.org2.neurashield.com:10051",
      "grpcOptions": {
        "ssl-target-name-override": "peer1.org2.neurashield.com"
      },
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org1.neurashield.com": {
      "url": "https://ca.org1.neurashield.com:7054",
      "caName": "ca-org1",
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric-ca-server/ca-cert.pem"
      },
      "httpOptions": {
        "verify": true
      }
    },
    "ca.org2.neurashield.com": {
      "url": "https://ca.org2.neurashield.com:8054",
      "caName": "ca-org2",
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric-ca-server/ca-cert.pem"
      },
      "httpOptions": {
        "verify": true
      }
    }
  }
}
