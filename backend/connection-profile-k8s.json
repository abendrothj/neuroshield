{
  "name": "neura-shield-network",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": ["peer0.org1.example.com"],
      "certificateAuthorities": ["ca.org1.example.com"]
    }
  },
  "peers": {
    "peer0.org1.example.com": {
      "url": "grpcs://fabric-peer:7051",
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric/tls/ca.crt"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org1.example.com": {
      "url": "https://fabric-ca:7054",
      "caName": "ca-org1",
      "tlsCACerts": {
        "path": "/etc/hyperledger/fabric-ca-server/ca-cert.pem"
      }
    }
  }
}

